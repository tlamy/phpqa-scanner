{
  "name": "tlamy/phpqa-scanner",
  "description": "Scans a project for issues and transmits them to phpqa-server",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Thomas Lamy",
      "email": "tlamy.me@gmail.com"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "require": {
    "php": "^7.3",
    "phpstan/phpstan": "^0.12.5",
    "vimeo/psalm": "^3.8",
    "rector/rector": "^0.6.10"
  },
  "autoload": {
    "psr-4": {
      "PhpQA\\Sanner\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
